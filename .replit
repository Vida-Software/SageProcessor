modules = ["python-3.11", "nodejs-20", "postgresql-16", "python3"]

[nix]
channel = "stable-24_05"
packages = ["cacert", "glibcLocales", "libyaml"]

[workflows]
runButton = "Run"

[[workflows.workflow]]
name = "Run"
author = 39710120
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"

[deployment]
run = ["sh", "-c", "npm run dev"]

[[ports]]
localPort = 3000
externalPort = 3000

[[ports]]
localPort = 5000
externalPort = 80